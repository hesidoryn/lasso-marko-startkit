{
  "name": "lasso-marko-startkit",
  "version": "1.0.0",
  "description": "Lasso + Marko Startkit",
  "repository": "https://github.com/schetnikovich/lasso-marko-startkit",
  "license": "ISC",

  "main": "src/server.js",
  "scripts": {
    "start": "./node_modules/.bin/browser-refresh src/server.js",
    "start-prod": "NODE_ENV=production ./node_modules/.bin/browser-refresh src/server.js",

    "marko": "./node_modules/.bin/markoc . --force",
    "marko-clean": "./node_modules/.bin/markoc . --clean",
    "build": "NODE_ENV=production node src/build.js"
  },

  "dependencies": {
    "async": "2.1.2",
    "compression": "1.6.2",
    "d3": "4.2.8",
    "express": "4.14.0",
    "jquery": "3.1.1",
    "lasso": "2.6.1",
    "lasso-less": "2.3.1",
    "lasso-marko": "2.0.7",
    "marko": "3.11.8",
    "marko-widgets": "6.3.7",
    "normalize.css": "5.0.0",
    "shortid": "2.2.6"
  },

  "devDependencies": {
    "browser-refresh-client": "1.1.4",
    "browser-refresh-taglib": "1.1.0",
    "browser-refresh": "1.6.0"
  }
}
