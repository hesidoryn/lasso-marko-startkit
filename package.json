{
  "name": "lasso-marko-startkit",
  "version": "1.0.0",
  "description": "Lasso + Marko Startkit",
  "repository": "https://github.com/schetnikovich/lasso-marko-startkit",
  "license": "ISC",
  "main": "src/server.js",
  "scripts": {
    "start": "./node_modules/.bin/browser-refresh src/server.js",
    "dev": "node src/server.js",
    "dev-br": "./node_modules/.bin/browser-refresh src/server.js",
    "prod": "NODE_ENV=production node src/server.js",
    "prod-br": "NODE_ENV=production ./node_modules/.bin/browser-refresh src/server.js",
    "marko": "./node_modules/.bin/markoc . --force",
    "marko-clean": "./node_modules/.bin/markoc . --clean",
    "build": "NODE_ENV=production node src/build.js",
    "lint": "./node_modules/.bin/eslint ."
  },
  "dependencies": {
    "async": "2.1.2",
    "compression": "1.6.2",
    "d3": "4.3.0",
    "express": "4.14.0",
    "jquery": "3.1.1",
    "lasso": "2.7.2",
    "lasso-less": "2.3.1",
    "lasso-marko": "2.0.7",
    "lasso-marko-extras": "1.1.0",
    "marko": "3.12.0",
    "marko-widgets": "6.3.7",
    "normalize.css": "5.0.0",
    "shortid": "2.2.6"
  },
  "devDependencies": {
    "browser-refresh": "1.6.0",
    "browser-refresh-client": "1.1.4",
    "browser-refresh-taglib": "1.1.0",
    "eslint": "3.9.1"
  }
}
